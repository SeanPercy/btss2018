{
  "name": "client",
  "version": "1.0.0",
  "license": "ISC",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "Apollo",
    "Babel",
    "Bootstrap",
    "GraphQL",
    "React",
    "SCSS",
    "TypeScript",
    "Webpack"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cc": "ts-node tools/cleanCache.ts",
    "prebuild": "npm run lint",
    "build": "cross-env NODE_ENV=production webpack",
    "pretest-build": "npm run build",
    "test-build": "ts-node test/testProductionBuild.ts",
    "server": "cross-env NODE_ENV=development webpack-dev-server",
    "server-autoreload": "nodemon --config tools/nodemon.json -x npm run server",
    "lint": "npm run lint:tsx && npm run lint:ts && npm run lint:style",
    "lint:tsx": "tslint -c src/tslint.json -p src/tsconfig.json --fix",
    "lint:style": "stylelint src/**/*.scss --config tools/.stylelintrc --cache --cache-location cache/.stylelintcache/",
    "lint:ts": "tslint -c tools/tslint.json -p tsconfig.json --fix"
  },
  "sideEffects": [
    "*css",
    "*.scss"
  ],
  "optionalDependencies": {
    "components": "tslint-hack to prevent a false warning getting emitted",
    "helpers": "tslint-hack to prevent a false warning getting emitted"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
